mixin stack(stackArray)
    ul.techs__list
        each value in stackArray
            li.techs__item.tech
                .tech__block
                    svg.tech__vector.circle(viewBox="0 0 110 110")
                        circle.circle__base(r="45" cx="50%" cy="50%" fill="none")
                        circle(class=`circle__color circle__color--disable circle__color--${value.percent}` r="45" cx="50%" cy="50%" transform="rotate(-90 55 55)")
                    .tech__text= value.tech
